<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="ERROR">
  <Appenders>
    <File name="error" fileName="error.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} -%msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="error" level="ERROR"/>
    </Root>
  </Loggers>
</Configuration>